{
  "name": "react-wxeap",
  "version": "4.2.0",
  "description": "cnwxsoft web framework",
  "scripts": {
    "lint": "eslint --ext .js src --fix",
    "pb": "npm run build && npm publish",
    "pb-beta": "npm run build && npm publish --tag=beta",
    "pb-rc": "npm run build && npm publish --tag=rc",
    "pb-cmd": "npm run build-cmd && npm publish",
    "build": "npm run lint && npm run transfer && npm run compile",
    "transfer": "npm run clean && npm run copy",
    "clean": "rm -r lib/*",
    "copy": "cp -r src/* lib && cp *.md docs && npm run docs",
    "docs": "mv docs/README.md docs/index.md",
    "compile": "babel src -d lib",
    "build-cmd": "npm run lint && npm run transfer-cmd && npm run compile",
    "transfer-cmd": "npm run clean-cmd && npm run copy-cmd",
    "copy-cmd": "xcopy src lib /y/s && xcopy *.md docs /y && npm run docs-cmd",
    "docs-cmd": "del docs\\index.md && rename docs\\README.md index.md",
    "clean-cmd": "rd /s/q lib && md lib"
  },
  "main": "lib/index",
  "typings": "lib/index.d.ts",
  "author": "cnwxsoft",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caigehui/react-wxeap.git"
  },
  "license": "MIT",
  "dependencies": {
    "@types/react": "^15.0.38",
    "@types/react-dom": "^15.5.1",
    "antd-mobile": "1.6.7",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.9.2",
    "dva": "^1.2.1",
    "lodash.clone": "^4.5.0",
    "mobile-detect": "^1.3.6",
    "moment": "2.18.1",
    "rc-form": "^1.3.2",
    "react": "~15.4.1",
    "react-dom": "~15.4.1",
    "react-photoswipe": "^1.3.0",
    "redux-persist": "~4.8.0",
    "jquery": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0"
  }
}
